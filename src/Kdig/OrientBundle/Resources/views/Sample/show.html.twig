{% extends app.request.xmlHttpRequest ? kdig.templates.full : kdig.templates.partial %}
{% block content %}
<div class="col-sm-1 left-menu">
    {% block content_left_menu %}
        {{ mopa_bootstrap_menu('leftmenu') }}
        {{ mopa_bootstrap_menu('vocsmenupottery') }}
    {% endblock content_left_menu %}
</div>
<div class="col-sm-11 center">
    {% block page_header %}
    <div class="page-header col-sm-12">
        {{ apy_breadcrumb_trail_render() }}
    </div>
    {% endblock page_header %}
    {% block content_content %}
        <div class="col-sm-12">
            <div class="control-group">
                <label class="control-label">Id</label>
                <div class="controls">                <col-sm- class="input-xlarge uneditable-input">{{ entity.id }}</col-sm->            </div>
            </div>        <div class="control-group">
                <label class="control-label">Remarks</label>
                <div class="controls">                <col-sm- class="input-xlarge uneditable-input">{{ entity.remarks }}</col-sm->            </div>
            </div>        <div class="control-group">
                <label class="control-label">Created</label>
                <div class="controls">                <col-sm- class="input-xlarge uneditable-input">{{ entity.created|date('Y-m-d H:i:s') }}</col-sm->            </div>
            </div>        <div class="control-group">
                <label class="control-label">Updated</label>
                <div class="controls">                <col-sm- class="input-xlarge uneditable-input">{{ entity.updated|date('Y-m-d H:i:s') }}</col-sm->            </div>
            </div>        <div class="control-group">
                <label class="control-label">Isactive</label>
                <div class="controls">                <col-sm- class="input-xlarge uneditable-input">{{ entity.isActive }}</col-sm->            </div>
            </div>        <div class="control-group">
                <label class="control-label">Ispublic</label>
                <div class="controls">                <col-sm- class="input-xlarge uneditable-input">{{ entity.isPublic }}</col-sm->            </div>
            </div>        <div class="control-group">
                <label class="control-label">Isdelete</label>
                <div class="controls">                <col-sm- class="input-xlarge uneditable-input">{{ entity.isDelete }}</col-sm->            </div>
            </div>      
        </div>
    {% endblock content_content %}
</div>
{% endblock content %}