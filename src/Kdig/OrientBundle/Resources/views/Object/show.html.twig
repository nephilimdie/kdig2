                <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.location }}</span>            </div>
            </div>        <div class="control-group">
                <label class="control-label">Museum_acquisition</label>
                <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.museumacquisition }}</span>            </div>
            </div>        <div class="control-group">
                <label class="control-label">Museum_acquisition_notes</label>
                <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.museumacquisitionnotes }}</span>            </div>
            </div>        <div class="control-group">
                <label class="control-label">Exhibition_history</label>
                <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.exhibitionhistory }}</span>            </div>
            </div>        <div class="control-group">
                <label class="control-label">Itaremarks</label>
                <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.itaremarks }}</span>            </div>
            </div>      
        </div>
    {% endblock content_content %}
</div>
{% endblock content %}