                <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.weight }}</span>            </div>
            </div>        <div class="control-group">
                <label class="control-label">Bibliography</label>
                <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.bibliography }}</span>            </div>
            </div>        <div class="control-group">
                <label class="control-label">Restorationdate</label>
                <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.restorationdate|date('Y-m-d H:i:s') }}</span>            </div>
