{% extends app.request.xmlHttpRequest ? kdig.templates.full : kdig.templates.partial %}
{% block title %}
    {{ 'views.new.creation'|trans({'%entity%': 'Photolist'}, 'JordiLlonchCrudGeneratorBundle') }}
{% endblock %}

{% block page %}

<p>
  <button class="btn add_photo" href="{{ path('photolist_new') }}" type="button">Add Photolist</button>
</p>

<div class="add_new_photolist row-fluid">
</div>

<script type="text/javascript">
$(document).ready( function () {
    $('.add_photo').on("click", function(event){ 
        event.preventDefault();
        getdefaultstringAjax ($(this).attr('href'), $('.add_new_photolist'), 'html')
    });
});
</script>
{% endblock %}