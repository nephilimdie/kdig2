{% extends app.request.xmlHttpRequest ? kdig.templates.full : kdig.templates.partial %}
{% block title %}
    {{ 'views.show.show'|trans({'%entity%': 'Photolist'}, 'JordiLlonchCrudGeneratorBundle') }}
{% endblock %}

{% block page %}

<h1>Photolist</h1>

<form class="form-horizontal">
    <fieldset>        <div class="control-group">
            <label class="control-label">Id</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.id }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Remarks</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.remarks }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Created</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.created|date('Y-m-d H:i:s') }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Updated</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.updated|date('Y-m-d H:i:s') }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Isactive</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.isActive }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Ispublic</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.isPublic }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Isdelete</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.isDelete }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Fromnumber</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.fromnumber }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Tonumber</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.tonumber }}</span>            </div>
        </div>    </fieldset>
</form>

<div>
    <div class="float-left">
        <a class="btn" href="{{ path('photolist') }}">
            {{ 'views.recordactions.backtothelist'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
        </a>
    </div>
    <div class="float-left">
        <a class="btn btn-primary" href="{{ path('photolist_edit', { 'id': entity.id }) }}">
            {{ 'views.recordactions.edit'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
        </a>
    </div>
    <div class="float-left">
        <form action="{{ path('photolist_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button class="btn btn-danger" type="submit" onclick="return confirm('{{ 'views.recordactions.confirm'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}');">{{ 'views.recordactions.delete'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}</button>
        </form>
    </div>
</div>
{% endblock %}